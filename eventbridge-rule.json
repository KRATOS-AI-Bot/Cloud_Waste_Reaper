
{
  "EventPattern": "rate(1 week)",
  "ScheduleExpression": "cron(0 9 ? * MON *)",
  "Targets": [
    {
      "Arn": "arn:aws:lambda:REGION:ACCOUNT_ID:function:kratos-lambda",
      "Id": "kratos-lambda"
    }
  ],
  "Name": "kratos-lambda-schedule",
  "State": "ENABLED",
  "Description": "Trigger kratos-lambda every Monday at 9:00am"
}
